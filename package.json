{
  "name": "bravado-client-generator",
  "version": "0.3.1",
  "description": "Code generator for Bravado API clients",
  "main": "lib/main.js",
  "bin": {
    "bravado-client-generator": "bin/generate"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn install",
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:changed": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=8.3"
  },
  "author": "Losant <hello@losant.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Losant/bravado-client-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Losant/bravado-client-generator.git"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "debug": "^4.1.0",
    "defaults": "^1.0.3",
    "fast-json-stable-stringify": "^2.0.0",
    "fs-extra": "^7.0.0",
    "handlebars": "^4.0.12",
    "inflection": "^1.12.0",
    "json-refs": "^3.0.12",
    "qs": "^6.5.2",
    "lodash": "^4.17.11",
    "uri-template": "^1.0.1"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.3.4",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node"
  }
}
