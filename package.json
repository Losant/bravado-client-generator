{
  "name": "bravado-client-generator",
  "version": "0.3.2",
  "description": "Code generator for Bravado API clients",
  "main": "lib/main.js",
  "bin": {
    "bravado-client-generator": "bin/generate"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn install",
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:changed": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=10"
  },
  "author": "Losant <hello@losant.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Losant/bravado-client-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Losant/bravado-client-generator.git"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "commander": "^4.0.0",
    "debug": "^4.1.1",
    "defaults": "^1.0.3",
    "fast-json-stable-stringify": "^2.0.0",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.5.1",
    "inflection": "^1.12.0",
    "json-refs": "^3.0.13",
    "qs": "^6.9.1",
    "lodash": "^4.17.15",
    "uri-template": "^1.0.1"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.3.10",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
