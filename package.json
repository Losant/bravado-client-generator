{
  "name": "bravado-client-generator",
  "version": "0.2.0",
  "description": "Code generator for Bravado API clients",
  "main": "lib/main.js",
  "bin": {
    "bravado-client-generator": "bin/generate"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn install",
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:changed": "lint-staged",
    "preinstall": "node -e 'if(!process.env.npm_execpath.includes(`yarn`)){console.error(`Use yarn!`);process.exit(1);}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=8.3"
  },
  "author": "Losant <hello@losant.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Losant/bravado-client-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Losant/bravado-client-generator.git"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.3.2",
    "commander": "^2.15.1",
    "debug": "^3.1.0",
    "defaults": "^1.0.3",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.11",
    "inflection": "^1.12.0",
    "json-refs": "^3.0.4",
    "qs": "^6.5.1",
    "lodash": "^4.17.5",
    "uri-template": "^1.0.1"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.3.4",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node"
  }
}
